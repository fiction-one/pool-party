    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "react": "18.2.0",
    "styled-components": "^5.3.9",
    "tsconfig": "workspace:*",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
